import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
     System.out.print("Enter a Number: ");
        int num = scanner.nextInt();
        
         int i = 1;
         int sum = 0;
         while (i <= num) {
             System.out.println(i);
        sum += i;
            i++;
         }

            System.out.println(sum);
           }
        }
